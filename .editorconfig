[*.cs]

# CS8618: O campo não anulável precisa conter um valor não nulo ao sair do construtor. Considere declará-lo como anulável.
dotnet_diagnostic.CS8618.severity = none

# CS8981: O nome do tipo contém apenas caracteres ascii em caixa baixa. Esses nomes podem ficar reservados para o idioma.
dotnet_diagnostic.CS8981.severity = none

# CS8604: Possível argumento de referência nula.
dotnet_diagnostic.CS8604.severity = none

# CS1591: O comentário XML ausente não foi encontrado para o tipo ou membro visível publicamente
dotnet_diagnostic.CS1591.severity = none

# CS8625: Não é possível converter um literal nulo em um tipo de referência não anulável.
dotnet_diagnostic.CS8625.severity = none
